  "main": "./build/app.js",
  "scripts": {
    "build": "npm run clean && npm run build-babel",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src --out-dir ./build --source-maps --copy-files",
    "start": "npm run build && node --require dotenv/config ./build/app.js",
    "start:dev": "nodemon --require dotenv/config --exec babel-node ./src/app.js"
  },
